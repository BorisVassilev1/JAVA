package edu.tutorial.application1;

public class Camera {
	private float x;
	private float y;
	private float z;
	private float rx;
	private float ry;
	private float rz;
	
	private float fov;
	private float aspect;
	private float near;
	private float far;
	
	public Camera() {
		x=0;
		y=0;
		z=0;
		rx=0;
		ry=0;
		rz=0;
		
		fov=0;
		aspect=0;
		near=0;
		far=0;
	}
}
